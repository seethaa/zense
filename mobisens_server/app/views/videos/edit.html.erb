<h1>Editing video</h1>

<% form_for(@video) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :timestamp %><br />
    <%= f.text_field :timestamp %>
  </p>
  <p>
    <%= f.label :annotation %><br />
    <%= f.text_field :annotation %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @video %> |
<%= link_to 'Back', videos_path %>