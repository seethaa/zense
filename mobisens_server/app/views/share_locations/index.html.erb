<h1>Listing share_locations</h1>

<table>
  <tr>
    <th>Device</th>
    <th>Latitude</th>
    <th>Longitude</th>
    <th>Timestamp</th>
  </tr>

<% @share_locations.each do |share_location| %>
  <tr>
    <td><%=h share_location.device_id %></td>
    <td><%=h share_location.latitude %></td>
    <td><%=h share_location.longitude %></td>
    <td><%=h share_location.timestamp %></td>
    <td><%= link_to 'Show', share_location %></td>
    <td><%= link_to 'Edit', edit_share_location_path(share_location) %></td>
    <td><%= link_to 'Destroy', share_location, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New share_location', new_share_location_path %>